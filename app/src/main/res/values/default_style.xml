<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- 默认提交按钮样式 -->
    <style name="default_textview_btn">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/button_height_44dp</item>
        <item name="android:textSize">@dimen/txt_f18</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginLeft">@dimen/default_margin_left</item>
        <item name="android:layout_marginRight">@dimen/default_margin_right</item>
        <item name="android:background">@drawable/selector_common_orange_btn_bg</item>
        <item name="android:textColor">@color/selector_defult_btn_txt</item>
    </style>
    <!-- 默认文本 例如:edittext左边的提示文字 -->
    <style name="default_textview_txt1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/font_size_12</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:textColor">@color/all_txt_32</item>
        <item name="android:textColorHint">@color/all_txt_b4</item>
    </style>

    <!-- edittext的默认样式 -->
    <style name="default_edit_style">
        <item name="android:layout_height">@dimen/base_item_height</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/edit_white_bg_normal</item>
        <item name="android:textColor">@color/all_txt_32</item>
        <item name="android:textColorHint">@color/all_txt_b4</item>
        <item name="android:textSize">@dimen/font_size_12</item>
    </style>

    <!-- 用油账本提交按钮样式 -->
    <style name="obook_textview_btn">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:textSize">@dimen/txt_f16</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:layout_marginRight">15dp</item>
        <item name="android:background">@drawable/selector_obook_btn_bg</item>
        <item name="android:textColor">@color/selector_defult_btn_txt</item>
    </style>

    <!-- 默认提交按钮样式 -->
    <style name="btn_gray_bg">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:textSize">@dimen/txt_f16</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginLeft">15dp</item>
        <item name="android:layout_marginRight">15dp</item>
        <item name="android:background">@color/msg_font_gray</item>
        <item name="android:textColor">@color/selector_defult_btn_txt</item>
    </style>

    <!-- 5.0默认黄色 直角按钮 -->
    <style name="default_five_textview_btn_bottom">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/button_height_44dp</item>
        <item name="android:textSize">@dimen/txt_f18</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/selector_yellow_btn_bg_bottom</item>
        <item name="android:textColor">@color/selector_defult_btn_txt</item>
    </style>

